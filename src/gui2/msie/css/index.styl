body
  background: #0ff
  // IE6 Fixed Position Jitter Fix
  -background-image: url(about:blank)
  -background-attachment: fixed
  margin: 0
  padding: 0
  margin-top: 1.5em

#head
  position: fixed
  -position: absolute
  top: 0
  -top: expression(eval(document\.documentElement\.scrollTop))
  left: 0
  width: 100%
  background: yellow
